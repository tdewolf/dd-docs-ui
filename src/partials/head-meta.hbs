{{#with page.attributes.meta-robots}}
<meta name="robots" content="{{this}}">
{{/with}}
{{#with page.description}}
<meta name="description" content="{{this}}">
{{/with}}
{{#with page.keywords}}
<meta name="keywords" content="{{this}}">
{{/with}}
{{#if page.component}}
<link rel="schema.dcterms" href="https://purl.org/dc/terms/">
{{#unless (eq page.attributes.pagination undefined)}}
    {{#with page.previous}}
        <link rel="prev" href="{{{relativize ./url}}}">
    {{/with}}
    {{#with page.next}}
        <link rel="next" href="{{{relativize ./url}}}">
    {{/with}}
{{/unless}}

{{#unless (eq page.attributes.pagination undefined)}}

    {{#if (or page.previous page.next)}}

        {{#if (eq page.attributes.pagination 'full') }}
            {{#with page.previous}}
                    <link rel="prev" href="{{{relativize ./url}}}">
            {{/with}}

        {{ else if (eq page.attributes.pagination '') }}
            {{#with page.previous}}
                <link rel="prev" href="{{{relativize ./url}}}">
            {{/with}}

        {{ else if (eq page.attributes.pagination 'prev') }}
            {{#with page.previous}}
                    <link rel="prev" href="{{{relativize ./url}}}">
            {{/with}}

        {{/if}}

        {{#if (eq page.attributes.pagination 'full') }}
            {{#with page.next}}
                    <link rel="next" href="{{{relativize ./url}}}">
            {{/with}}

        {{ else if (eq page.attributes.pagination '') }}
            {{#with page.next}}
                <link rel="next" href="{{{relativize ./url}}}">
            {{/with}}

        {{ else if (eq page.attributes.pagination 'next') }}
            {{#with page.next}}
                    <link rel="next" href="{{{relativize ./url}}}">
            {{/with}}

        {{/if}}
    {{/if}}
{{/unless}}

<meta name="dcterms.subject" content="{{page.component.name}}">
<meta name="dcterms.identifier" content="{{page.version}}">
<meta name="page-url" content="{{page.url}}">
<meta name="page-nav-header-levels" content="{{or page.attributes.nav-header-levels 0}}">

{{! meta tags starting docsearch: are automatically extracted by Algolia }}
{{! docsearch:version has special handling so we use `cversion` }}
<meta name="docsearch:component" content="{{page.component.name}}">
<meta name="docsearch:component_title" content="{{page.component.title}}">
<meta name="docsearch:cversion" content="{{page.version}}">
<meta name="docsearch:component_version" content="{{page.component.name}}@{{page.version}}">
<meta name="docsearch:module" content="{{#with page.attributes.module}}{{#if (ne this "ROOT")}}{{this}}{{/if}}{{/with}}">
<meta name="docsearch:breadcrumbs" content="{{> crumbs-meta}}">
<meta name="docsearch:topic_type" content="{{page.attributes.topic-type}}">
<meta name="docsearch:version_rank" content="{{page.attributes.version-rank}}">
<meta name="docsearch:status" content="{{page.attributes.status}}">
<meta name="docsearch:edition" content="{{page.attributes.edition}}">


{{#with page.attributes.meta-zd-site-verification}}
<meta name="zd-site-verification" content="{{this}}">
{{/with}}
{{/if}}
<meta name="generator" content="Antora {{antoraVersion}}">
<link rel="icon" href="{{{uiRootPath}}}/img/favicon.svg" type="image/svg+xml">
<link rel="icon" href="{{{uiRootPath}}}/img/favicon.ico" type="image/x-icon" sizes="any">
